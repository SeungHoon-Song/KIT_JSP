/*datepicer 버튼 롤오버 시 손가락 모양 표시*/
.ui-datepicker-trigger{cursor: pointer;}
/*datepicer input 롤오버 시 손가락 모양 표시*/
.hasDatepicker{cursor: pointer;}


.double {
	display: flex;
	width: 30rem;
}

.callDiv {
	width: 80%;
}
/* ------------------- 아이폰 미니 사이즈 --------------------- */
@media screen and (max-width: 667px){ 

	.imgDiv {
		width: 10%;
	}
	.imgDiv2-Btn {
		width: 100%;
	}
	a.button{
		width: 50%;
		margin-right: 10%;
	}

	.titleDiv2-b {
		width: 100%;
	}
	.contentDiv2 {
		width: 100%;
	}

	.locationDiv {
		width: 15%;
	}
	.locationBtn {
		width: 100%;
	}
	.callDiv {
		width: 100%;
	}
	
	.double {
		width: 100%;
		display: block;
	}
	/* #datepicker1, #datepicker2{
		width: 100%;
	}	 */
}
/* ------------------- 아이폰 se 사이즈 --------------------- */
@media screen and (max-width: 568px){ 

	.imgDiv {
		width: 15%;
	}
	.imgDiv2-Btn {
		width: 100%;
	}
	a.button{
		width: 50%;
		margin-right: 10%;
	}

	.titleDiv2-b {
		width: 100%;
	}
	.contentDiv2 {
		width: 100%;
	}

	.locationDiv {
		width: 15%;
	}
	.locationBtn, .callDiv {
		width: 100%;
	}
	
	.double {
		width: 100%;
		display: block;
	}
	/* #datepicker1, #datepicker2{
		width: 100%;
	}	 */
}
 
