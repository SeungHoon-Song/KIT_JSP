.imgdiv2-li span {
	font-size: small; color: red; font-family:sans-serif;
}

.titleDiv {
	font-size: 18px;
	width: 10%;
}
div > span {
	color: rgb(255, 80, 88);
	font-size: 12px;
	vertical-align: top;
}

.titleDiv2-a{
	width: 100%;
	-webkit-box-align: center;
	align-items: center;
}
.titleDiv2-b {
	display: flex;
	-webkit-box-align: center;
	align-items: center;
	width: 80%;
}
 

/* #datepicker1, #datepicker2{
	width: 30% ;
	
	cursor: pointer;
} */


.titleDiv2-c {
	margin-left: 1.5rem;
	font-size: 1rem;
	text-align: right;
}
.contentDiv{
	width: 10%;
}
.contentDiv2{
	width: 80%;

}

.locationDiv {
	width: 10%;
	font-size: 18px;
	padding-top: 14px;
}
.locationBtn {
	width: 80%;
}

