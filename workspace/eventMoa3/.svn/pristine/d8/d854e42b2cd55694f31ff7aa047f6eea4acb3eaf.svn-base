.guideText {
	margin-top: 1.5rem;
    color: rgb(74, 164, 255);
    line-height: 1.5;
    font-size: 14px;
}
.sc-div img {
	height: 200px;
	width: 200px;
}

.leaderImg {
	position: absolute;
    height: 1.5rem;
    padding: 0px 0.5rem;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    top: 0.5rem;
    left: 0.5rem;
    background-color: rgba(30, 29, 41, 0.32);
    color: rgb(255, 255, 255);
    border-radius: 12px;
    font-size: 12px;
}
.deleteImg {
    width: 1.5rem;
    height: 1.5rem;
    background-position: center center;
    background-repeat: no-repeat;
    background-size: 12px 12px;
    background-image: url(data:image/svg+xml;base64,);
    background-color: rgba(30, 29, 41, 0.32);
    border-radius: 50%;
    position: absolute;
    top: 0.5rem;
    right: 0.5rem;
}

