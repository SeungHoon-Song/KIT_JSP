.imgdiv2-li span {
	font-size: small; color: red; font-family:sans-serif;
}

.titleDiv {
	font-size: 18px;
	padding-top: 14px;
}
div > span {
	color: rgb(255, 80, 88);
	font-size: 12px;
	vertical-align: top;
}

.titleDiv2-a{
	width: 100%;
	-webkit-box-align: center;
	align-items: center;
}
.titleDiv2-b {
	display: flex;
	-webkit-box-align: center;
	align-items: center;
}
.titleDiv2-b input {
	width: 600px;
}

.dayDiv > input {
	width: 20%;
}

@media screen and (max-width: 660px){
	.titleDiv2-b input {
		width: 100%;
	}	
}
.titleDiv2-c {
	margin-left: 1.5rem;
	font-size: 1rem;
	text-align: right;
}

.contentDiv2{
	width: 100%;
}

.locationDiv {
	width: 10.5rem;
	font-size: 18px;
	padding-top: 14px;
}